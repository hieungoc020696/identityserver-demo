services:
  api:
    build:
      context: D:\Projects\IdentityServer\identityserver-demo
      dockerfile: Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Api
      target: base
    container_name: Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: docker.debugger.vn/api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: {}
    ports:
    - published: 5000
      target: 80
    tty: true
    volumes:
    - D:\Projects\IdentityServer\identityserver-demo\Api:/app:rw
    - C:\Users\HIEU NGOC\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\HIEU NGOC\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\HIEU NGOC\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\HIEU NGOC\.nuget\packages:/root/.nuget/packages:ro
    - D:\Projects\IdentityServer\identityserver-demo:/src:rw
  identity-api:
    build:
      context: D:\Projects\IdentityServer\identityserver-demo
      dockerfile: IdentityServerDemo/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: IdentityServerDemo
      target: base
    container_name: IdentityServerDemo
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: docker.debugger.vn/identity-api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/IdentityServerDemo.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: {}
    ports:
    - published: 5001
      target: 80
    tty: true
    volumes:
    - D:\Projects\IdentityServer\identityserver-demo\IdentityServerDemo:/app:rw
    - C:\Users\HIEU NGOC\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\HIEU NGOC\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\HIEU NGOC\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\HIEU NGOC\.nuget\packages:/root/.nuget/packages:ro
    - D:\Projects\IdentityServer\identityserver-demo:/src:rw
  postgresql-data:
    environment:
      POSTGRES_PASSWORD: DigiMed123
    image: postgres:10
    networks:
      default: {}
    ports:
    - published: 5442
      target: 5432
    volumes:
    - postgres-data:/var/lib/postgresql/data:rw
version: '3.4'
volumes:
  postgres-data:
    external: true
    name: postgres-data